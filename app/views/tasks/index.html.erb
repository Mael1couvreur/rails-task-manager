<h1>List of all tasks</h1>


<% @tasks.each do |task| %>

  <% if task.completed %>
    </p><i class="far fa-check-circle"></i> <%= link_to task.title, task_path(task)%><p>
  <% else %>
    </p><i class="far fa-circle"></i> <%= link_to task.title, task_path(task)%><p>
  <% end %>

<% end %>

<%= link_to "Create new task", new_restaurant_path %>
